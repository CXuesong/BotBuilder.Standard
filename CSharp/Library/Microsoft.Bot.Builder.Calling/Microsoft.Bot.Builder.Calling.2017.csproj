<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net46;netstandard2.0</TargetFrameworks>
    <AssemblyName>Microsoft.Bot.Builder.Calling</AssemblyName>
    <RootNamespace>Microsoft.Bot.Builder.Calling</RootNamespace>
    <PackageId>CXuesong.Ported.Microsoft.Bot.Builder.Calling</PackageId>
    <Product>Microsoft Bot Builder SDK</Product>
    <Version>3.12.2.5</Version>
    <Authors>Microsoft, CXuesong</Authors>
    <Description>
      This is an unofficial .NET Core 2.0/.NET Framework 4.5 ported version of Microsoft.Bot.Builder.Calling package.

      Microsoft Bot Builder extension for Skype Calling API. It provides the required constructs to extend your bot to handle incoming calls from Skype.
    </Description>
    <PackageProjectUrl>https://github.com/CXuesong/BotBuilder.Standard</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CXuesong/BotBuilder.Standard</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <NeutralLanguage>en-US</NeutralLanguage>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <NoWarn>0419;1591;1701;1702;1705;1998</NoWarn>
  </PropertyGroup>
  
  <ItemGroup>
    <Compile Remove="Properties\**\*" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Microsoft.Bot.Builder\Microsoft.Bot.Builder.2017.csproj" />
    <ProjectReference Include="..\Microsoft.Bot.Builder.Autofac\Microsoft.Bot.Builder.Autofac.2017.csproj" />
  </ItemGroup>

</Project>